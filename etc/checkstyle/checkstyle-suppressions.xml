<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE suppressions PUBLIC "-//Puppy Crawl//DTD Suppressions 1.1//EN" "http://www.puppycrawl.com/dtds/suppressions_1_1.dtd">

<suppressions>
	<!-- Don't check generated source files -->
	<suppress files=".*[/\\]src[/\\].*[/\\]gen-src[/\\].*" checks=".*"/>
	<suppress files=".*[/\\]src[/\\].*[/\\]gen-test-src[/\\].*" checks=".*"/>
	
	<!-- Only check property file checks on properties files -->
	<suppress files="\A.*(?&lt;!\.properties)\z" id="propfileATGLoggingDebugOn" />
	<suppress files="\A.*(?&lt;!\.properties)\z" id="propfileTODO" />
</suppressions>
